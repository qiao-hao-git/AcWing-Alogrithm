#include <iostream>

using namespace std;

const int N = 1e5 + 10;

int son[N][26], cnt[N], idx;

char p[N];

int n;
char c;

void insert(char str[])
{
    int p = 0;
    for(int i = 0; str[i]; i ++)
    {
        int a = str[i] - 'a';
        if(!son[p][a]) son[p][a] = ++idx;
        p = son[p][a];
    }
    cnt[p] ++;
}

int query(char str[])
{
    int p = 0;
    for(int i = 0; str[i]; i ++)
    {
        int a = str[i] - 'a';
        if(!son[p][a]) return 0;
        p = son[p][a];
    }
    return cnt[p];
}

int main()
{
    cin >> n;
    while(n --)
    {
        cin >> c >> p;
        if(c == 'I') insert(p);
        else cout << query(p) << endl;
    }
    return 0;
}
