#include <iostream>

using namespace std;

const int N = 1e5 + 10;
int n, m;
int p[N];

int find(int x)
{
    if(x == p[x]) return p[x];
    else return p[x] = find(p[x]);
}

void union_(int x, int y)
{
    int fx = find(x);
    int fy = find(y);
    p[fx] = fy;
}
int main()
{
    cin >> n >> m;
    for(int i = 1; i <= n; i ++) p[i] = i;
    char c;
    int a, b;
    while(m --)
    {
        cin >> c >> a >> b;
        if(c == 'M' ) 
        {
            union_(a, b);
        }
        else  
        {
            if(find(a) == find(b)) cout << "Yes" << endl;
            else cout << "No" << endl;
        }
    }
    return 0;
}
