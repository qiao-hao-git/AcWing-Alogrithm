#include <iostream>
using namespace std;

typedef long long LL;

const int N = 1e5 + 10;

int q[N], tmp[N];
int n;

LL m_sort(int q[], int l, int r)
{
    if(l >= r) return 0;
    int mid = (l + r) >> 1;
    LL res = m_sort(q, l, mid) + m_sort(q, mid + 1, r);
    int i = l, j = mid + 1, k = 0;
    while(i <= mid && j <= r)
    {
        if(q[i] <= q[j]) tmp[k ++] = q[i ++];
        else{
            res += mid - i + 1;
            tmp[k ++] = q[j ++];
        }
    }
    while(i <= mid) tmp[k ++] = q[i ++];
    while(j <= r) tmp[k ++] = q[j ++];
    for(i = l, j = 0; i <= r; i ++, j ++) q[i] = tmp[j];
    return res;
}
int main()
{
    scanf("%d", &n);
    for(int i = 0; i < n; i ++) scanf("%d", &q[i]);
    printf("%lld", m_sort(q, 0, n - 1));
    return 0;
}



