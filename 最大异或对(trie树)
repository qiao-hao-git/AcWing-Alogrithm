#include <iostream>

using namespace std;

const int N = 1e5 + 10, M = 31 * N;

int a[N], s[M][2], idx;

int n;

void insert(int x)
{
    int p = 0;
    for(int i = 30; i >= 0; i --)
    {
        int u = x >> i & 1;
        if(!s[p][u]) s[p][u] = ++idx;
        p = s[p][u];
    }
}


int query(int x)
{
    int res = 0, p = 0;
    for(int i = 30; i >= 0; i --)
    {
        int u = x >> i & 1;
        if(s[p][!u]) 
        {
            res += 1 << i;
            p = s[p][!u];
        }
        else {
            p = s[p][u];
        }
    }
    return res;
}
int main()
{
    cin >> n;
    for(int i = 0; i < n; i ++)
    {
        cin >> a[i];
        insert(a[i]);
    }
    int res = 0;
    for(int i = 0; i < n; i ++)
    {
        res = max(res, query(a[i]));
    }
    cout << res << endl;
    return 0;
}
