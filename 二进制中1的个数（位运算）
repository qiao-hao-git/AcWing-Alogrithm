#include <iostream>

using namespace std;
const int N = 1e5 + 10;

long long a[N];
long long b[N];
int n;

int main()
{
    scanf("%d", &n);
    for(int i = 0; i < n; i ++) scanf("%lld", &a[i]);
    for(int i = 0; i < n; i ++)
    {
        while(a[i] != 0)
        {
            a[i] -= (a[i] & (-a[i]));
            b[i] ++;
        }
    }
    for(int i = 0; i < n; i ++) printf("%lld ", b[i]);
    return 0;
}
