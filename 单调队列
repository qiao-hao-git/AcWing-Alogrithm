#include <iostream>

using namespace std;

const int N = 1e6 + 10;

int n, k;

int a[N], q[N];

int main()
{
    cin >> n >> k;
    for(int i = 0; i < n; i ++) scanf("%d", &a[i]);
    int top = 0, tail = -1;
    for(int i = 0; i < n; i ++)
    {
        if(top <= tail && i - k + 1 > q[top]) top ++;
        while(top <= tail && a[q[tail]] >= a[i]) tail --;
        q[++ tail] = i;
        if(i >= k - 1) printf("%d ", a[q[top]]);
    }
    printf("\n");
    top = 0, tail = -1;
    for(int i = 0; i < n; i ++)
    {
        if(top <= tail && i - k + 1 > q[top]) top ++;
        while(top <= tail && a[q[tail]] <= a[i]) tail --;
        q[++ tail] = i;
        if(i >= k - 1) printf("%d ", a[q[top]]);
    }
    printf("\n");
    
    return 0;
}
