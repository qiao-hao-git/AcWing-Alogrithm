#include <iostream>

using namespace std;

double n;
int main()
{

    scanf("%lf", &n);
    if(n >= 0){
        double l = 0, r = 100;
        while(r - l > 1e-8){
        double mid = (l + r) / 2;
        if(mid * mid * mid >= n) r = mid;
        else l = mid;
        }
        printf("%.6lf\n", l);
    }
    else{
        double l = -100, r = 0;
        while(r - l > 1e-8){
            double mid = (l + r) / 2;
            if(mid * mid * mid >= n) r = mid;
            else l = mid;
        }
        printf("%.6lf\n", l);
    }
    return 0;
}
