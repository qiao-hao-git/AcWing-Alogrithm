#include<iostream>
using namespace std;

const int N = 1e5 + 10;
int a[N], b[N];

int n;

int main()
{
    cin >> n;
    for(int i = 0; i < n; i ++) cin >> a[i];
    int res = 0;
    for(int i = 0, j = 0; i < n; i ++)
    {
        b[a[i]] ++;
        while(j <= i && b[a[i]] > 1)
        {
            b[a[j]] --;
            j ++;
        }
        res = max(res, i - j + 1);
    }
    cout << res << endl;
    return 0;
}
