#include <iostream>

using namespace std;

const int N = 1e5 + 10;

int n, m;
int l, r;
int q[N], s[N];
int main()
{
    cin >> n >> m;
    for(int i = 1; i <= n; i ++) cin >> q[i];
    for(int i = 1; i <= n; i ++) s[i] = s[i - 1] + q[i];
    while(m -- )
    {
        cin >> l >> r;
        cout << s[r] - s[l - 1] << endl;
    }
    return 0;
}
