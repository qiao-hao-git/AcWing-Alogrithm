#include<iostream>
#include<vector>

using namespace std;
int main() {
    int n, v;
    cin >> n >> v;
    vector<int> values(n, 0);
    vector<int> weights(n, 0);
    vector<int> dp(v + 1, 0);
    for(int i = 0; i < n; i++) {
        cin >> weights[i] >> values[i];
    }
    for(int i = 0; i < n; i++) {
        for(int j = v; j >= weights[i]; j--) {
            dp[j] = max(dp[j], dp[j - weights[i]] + values[i]);
        }
    }
    cout << dp[v];
    return 0;
}
